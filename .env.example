# N8N host. If running locally, use localhost:<port>.
N8N_HOST=<your-host> # Example: n8n.example.com

# Webhook URL to be used by n8n. Must be a valid URL. If running locally, use http://localhost:<port>.
WEBHOOK_URL=<your-webhook-url> # Example: https://n8n.example.com/

# The level of logging to be output
N8N_LOG_LEVEL=debug

# The timezone to use. Must be a valid timezone.
GENERIC_TIMEZONE=Europe/Paris

# The password to access the Redis database. If changed, update the Redis credential inside n8n platform.
REDIS_PASSWORD=<redis_password>

# The password to access the Postgres database. Can be any password you want. Make it secure.
DB_POSTGRESDB_PASSWORD=<postgres_password>

# The password that will encrypt n8n credentials. Can be any password you want. Make it secure.
N8N_ENCRYPTION_KEY=<random-key>